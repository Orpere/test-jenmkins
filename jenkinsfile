pipeline {
  agent { label 'linux' }
  environment {
   KUBECONFIG=credentials('kubeid') 
  }
  stages {
    stage('dev') {
      steps {
        sh 'sudo apt-get install -y ansible'
        sh 'ansible-galaxy collection install -r requirements.yml'
        sh 'ansible-playbook test-playbook.yaml  --extra-vars config=$KUBECONFIG'
      }
    }
  }
}